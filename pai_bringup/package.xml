  <exec_depend>so_arm100_description</exec_depend>
  <exec_depend>so_arm101_description</exec_depend>
  <exec_depend>so_arm_gz</exec_depend>
  <exec_depend>std_msgs</exec_depend>
  <exec_depend>xacro</exec_depend>

  <export>
    <build_type>ament_cmake</build_type>
